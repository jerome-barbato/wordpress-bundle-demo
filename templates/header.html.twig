{% if menu.footer|default() and menu.footer.items|length %}

	<nav class="footer-navigation" aria-label="{{ _e( 'Footer Menu', 'twentynineteen' ) }}">
		<ul id="footer" class="footer-menu">
			{% for item in menu.footer.items %}
				<a href="{{ item.link }}" class="{{ item.class }}" target="{{ item.target }}">{{ item.title }}</a>
			{% endfor %}
		</ul>
	</nav><!-- .footer-navigation -->
{% endif %}